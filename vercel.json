{
    "version": 2,
    "name": "socketing",
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/.*",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE"
            ],
            "headers": {
                "Access-Control-Allow-Origin": "https://to-do-next-iota.vercel.app",
                "Access-Control-Allow-Credentials": "true"
            }
        }
    ]
}